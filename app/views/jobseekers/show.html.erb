<% provide(:title, "Profile Page") %>
<h1>Welcome to your profile page!</h1>
<h2>
  Name: <%= @jobseeker.name %>
  <br>
  Email: <%= @jobseeker.email %>
</h2>
<div>
  <h2>
    Description:
    <br>
    <%= @jobseeker.description %>
  </h2>
  
</div>
<div>
  <h3>Skills:</h3>
  <% @jobseeker.skills.each do |skill| %>
    <%= skill.name %>
    <br>
  <% end %>
</div>